<?xml version="1.0" encoding="UTF-8"?>
<web-app metadata-complete="false" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name>code</display-name>
  <listener>
    <listener-class>me.zzp.code.setup.Database</listener-class>
  </listener>
  <listener>
    <listener-class>me.zzp.code.setup.Services</listener-class>
  </listener>
  <servlet>
    <servlet-name>static-files</servlet-name>
    <servlet-class>org.apache.catalina.servlets.DefaultServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>static-files</servlet-name>
    <url-pattern>/images/*</url-pattern>
    <url-pattern>/styles/*</url-pattern>
    <url-pattern>/scripts/*</url-pattern>
    <url-pattern>/statics/*</url-pattern>
  </servlet-mapping>
</web-app>
